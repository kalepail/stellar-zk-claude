# Bind address
API_BIND_ADDR=0.0.0.0:8080

# Logging
RUST_LOG=info

# Security and resource limits
API_KEY=
MAX_TAPE_BYTES=2097152
MAX_JOBS=64
MAX_FRAMES=18000
MIN_SEGMENT_LIMIT_PO2=16
MAX_SEGMENT_LIMIT_PO2=22
HTTP_MAX_CONNECTIONS=25000
HTTP_KEEP_ALIVE_SECS=75
# HTTP_WORKERS=4

# Job retention
JOB_TTL_SECS=86400
JOB_SWEEP_SECS=60

# Keep disabled for production
ALLOW_DEV_MODE_REQUESTS=false
ALLOW_UNVERIFIED_RECEIPTS=false
RISC0_DEV_MODE=0
