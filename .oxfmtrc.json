{
  "$schema": "https://raw.githubusercontent.com/nicolo-ribaudo/oxfmt-oxlint-tailwindcss/refs/heads/main/.oxfmtrc.schema.json",
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "lf"
}
