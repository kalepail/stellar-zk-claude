{
  "name": "stellar-zk",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "wrangler deploy",
    "typegen": "WRANGLER_LOG_PATH=.wrangler/logs wrangler types",
    "typegen:check": "WRANGLER_LOG_PATH=.wrangler/logs wrangler types --check",
    "typecheck:app": "tsc --noEmit -p tsconfig.json",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json",
    "typecheck:worker": "tsc --noEmit -p worker/tsconfig.json",
    "typecheck": "npm run typegen:check && npm run typecheck:app && npm run typecheck:node && npm run typecheck:worker",
    "lint": "oxlint src worker",
    "lint:fix": "oxlint src worker --fix",
    "format": "oxfmt src worker --write",
    "format:check": "oxfmt src worker --check",
    "check": "npm run typecheck && npm run lint && npm run format:check"
  },
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.23.0",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.11",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "wrangler": "^4.62.0"
  }
}
